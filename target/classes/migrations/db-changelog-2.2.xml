<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.27.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">

    <!-- Переименование таблицы users_bookings в users_booking -->
    <changeSet id="1742852273469-2" author="Kostya">
        <preConditions>
            <tableExists tableName="users_bookings"/>
            <not>
                <tableExists tableName="users_booking"/>
            </not>
        </preConditions>

        <renameTable
                oldTableName="users_bookings"
                newTableName="users_booking"
                catalogName="public"
                schemaName="public"/>

        <comment>Переименовал таблицу users_bookings в users_booking</comment>
    </changeSet>

</databaseChangeLog>